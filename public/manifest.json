{
  "name": "Works of Art",
  "version": "0.1.8",
  "manifest_version": 2,
  "description": "Each time you open a new tab, Works of Art presents an object from London's Victoria and Albert Museum collection",
  "homepage_url": "",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "options_ui": {
      "page": "src/options/index.html",
      "chrome_style": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Page action",
    "default_popup": "src/page_action/page_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://www.vam.ac.uk/*",
    "https://www.pinterest.com/*",
    "http://assets.pinterest.com/*"
  ]
}