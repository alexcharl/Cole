{
  "name": "V&A object in every tab",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Each time a tab opens, a wonderful object from the V&A collection appears",
  "homepage_url": "http://gjover.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "options_ui": {
      "page": "src/options/index.html",
      "chrome_style": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "tabs",
    "geolocation",
    "storage",
    "http://www.vam.ac.uk/*",
    "https://www.pinterest.com/*",
    "http://assets.pinterest.com/*"
  ]
}