{
  "name": "Works of Art",
  "version": "0.1.8.1",
  "manifest_version": 2,
  "description": "Discover objects from the Victoria and Albert museumâ€™s collection every time you open a new tab",
  "homepage_url": "http:/alexcharlton.co",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "default_locale": "en",
  "offline_enabled": false,
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "options_ui": {
      "page": "src/options/index.html",
      "chrome_style": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Page action",
    "default_popup": "src/page_action/page_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://www.vam.ac.uk/*",
    "https://*.pinterest.com/*",
  ]
}